                        <Grid x:Name="MainViewContentControl"
                              Grid.Row="2"
                              Margin="18">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="25"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <StackPanel>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="25"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="120"/>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="90"/>
                                    </Grid.ColumnDefinitions>
                                    <Label Content="Revisión" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Foreground="White" FontSize="14" Margin="5,0,0,0" FontWeight="Bold" VerticalContentAlignment="Top" FontFamily="Neo Tech Std"/>
                                    <ComboBox x:Name="Revisión" Grid.Row="0" Grid.Column="1"  SelectedIndex="0" Foreground="{StaticResource General2}" FontSize="14" Margin="0,1,25,1" FontWeight="Medium" VerticalContentAlignment="Top" HorizontalContentAlignment="Center" FontFamily="Neo Tech Std">
                                        <ComboBoxItem Content="2VR4"/>
                                        <ComboBoxItem Content="2VR5"/>
                                    </ComboBox>
                                    <Label Content="Nº Strings" Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="2" Foreground="White" FontSize="14" Margin="5,0,0,0" FontWeight="Bold" VerticalContentAlignment="Top" FontFamily="Neo Tech Std"/>
